                background-color: white !important;
            }
            .slds-input{
                width:30%;
            }
            .mainTitle{
                font-size: 31px;
                margin-left: 27px;
            }
            #rcorners2 {
                border-radius: 10px;
                border: 2px solid #ff0000;
                padding: 20px; 
                width: 70%;
                height: auto;    
            }
        
            .custom-radio label {
                font-size: 12px;
                margin: 0px;
                font-family: 'Salesforce Sans';
                font-weight: bold;
            }
            
            .custom-radio input[type="radio"] {
                transform: scale(1.5); /* Increase the size of the radio button */
                margin-right: 12px; /* Space between the radio button and text */
            }

            th.headerRow {
                font-family: 'Salesforce Sans';
                font-size: 12px !important ;
            }
        </style>
    
        <!-- LIGHTNING DESIGN COMPONENT FROM STATIC RESORUCE -->
        
        <apex:stylesheet value="{!URLFOR($Resource.SLDS1050, 'styles/salesforce-lightning-design-system.min.css')}" />
            <head>
                <apex:includeScript value="{!$Resource.JqueryMinJs}"/>
                <apex:stylesheet value="{!$Resource.BootstrapCSS}"/>
                <apex:includeScript value="{!$Resource.GithubSelect2}"/>
                <!--  jQuery -->
                <apex:includeScript value="{!$Resource.CodeJquery}"/>
