            <apex:outputPanel style="font-weight: bold;font-size:15px;font-family:Salesforce Sans;">
                Please use below filter conditions to filter records which is eligible for automation (Ex: If OpportunityAmount > 1000)
            </apex:outputPanel>
            <br/>
            <br/>
            <!-- FILTER CODE STARTS FROM HERE -->
            <apex:pageBlockTable value="{!tableRows}" var="row" id="table">

            <!-- Field Name Dropdown -->
            <apex:column headerValue="Field Name">
                <apex:selectList value="{!row.fieldName}" style="width: -webkit-fill-available;" size="1" disabled="{!row.index > enabledRows}">
                    <apex:selectOptions value="{!fieldOptions}" />
                    <apex:actionSupport event="onchange" action="{!updateRow}" rerender="table" >
                        <apex:param name="rowIndex" value="{!row.index}" assignTo="{!rowIndex}" />
                    </apex:actionSupport>
                </apex:selectList>
            </apex:column>

            <!-- Operator Dropdown -->
            <apex:column headerValue="Operator">
                <apex:outputPanel id="Operator">
                    <apex:selectList value="{!row.selectedOperator}" size="1" style="width: -webkit-fill-available;" disabled="{!row.index > enabledRows}">
                        <apex:selectOptions value="{!row.operators}" />
                    </apex:selectList>
                </apex:outputPanel>
            </apex:column>

            <!-- Value Field -->
            <apex:column headerValue="Value">
                <apex:outputPanel id="valuePanel">
                    <apex:inputText value="{!row.selectedValue}" style="width: -webkit-fill-available;" rendered="{!ISNULL(row.fieldType) || row.fieldType == ''}" disabled="{!row.index > enabledRows}" />
                    <apex:inputText value="{!row.selectedValue}" style="width: -webkit-fill-available;" rendered="{!row.fieldType == 'text' || row.fieldType == 'number'}" disabled="{!row.index > enabledRows}">
                        <!-- <apex:actionSupport event="onchange" action="{!enableNextRow}" rerender="table">
                            <apex:param name="rowIndex" value="{!row.index}" assignTo="{!rowIndex}" />
                        </apex:actionSupport> -->
                    </apex:inputText>

                    <apex:input type="date" style="width: -webkit-fill-available;"
                    value="{!filterDate1}"
                    rendered="{!row.fieldType = 'date' && row.index == 0}"
                    onchange="updatevalueJS(this.value, {!JSENCODE(row.indexAsString)})"
                    disabled="{!row.index > enabledRows}" ></apex:input>

                    <apex:input type="date" style="width: -webkit-fill-available;"
                    value="{!filterDate2}"
                    rendered="{!row.fieldType = 'date' && row.index == 1}"
                    onchange="updatevalueJS(this.value, {!JSENCODE(row.indexAsString)})"
                    disabled="{!row.index > enabledRows}" ></apex:input>

                    <apex:input type="date" style="width: -webkit-fill-available;"
                    value="{!filterDate3}"
                    rendered="{!row.fieldType = 'date' && row.index == 2}"
                    onchange="updatevalueJS(this.value, {!JSENCODE(row.indexAsString)})"
                    disabled="{!row.index > enabledRows}" ></apex:input>

                    <apex:input type="date" style="width: -webkit-fill-available;"
                    value="{!filterDate4}"
                    rendered="{!row.fieldType = 'date' && row.index == 3}"
                    onchange="updatevalueJS(this.value, {!JSENCODE(row.indexAsString)})"
                    disabled="{!row.index > enabledRows}" ></apex:input>

                    <apex:input type="date" style="width: -webkit-fill-available;"
                    value="{!filterDate5}"
                    rendered="{!row.fieldType = 'date' && row.index == 4}"
                    onchange="updatevalueJS(this.value, {!JSENCODE(row.indexAsString)})"
                    disabled="{!row.index > enabledRows}" ></apex:input>

                    <apex:input type="date" style="width: -webkit-fill-available;"
                    value="{!filterDate6}"
                    rendered="{!row.fieldType = 'date' && row.index == 5}"
                    onchange="updatevalueJS(this.value, {!JSENCODE(row.indexAsString)})"
                    disabled="{!row.index > enabledRows}" ></apex:input>

                    <apex:inputCheckbox value="{!row.selectedValue}"  rendered="{!row.fieldType == 'checkbox'}" disabled="{!row.index > enabledRows}">
                        <apex:actionSupport event="onchange" action="{!enableNextRow}" rerender="table">
                            <apex:param name="rowIndex" value="{!row.index}" assignTo="{!rowIndex}" />
                        </apex:actionSupport>
                    </apex:inputCheckbox>

                    <!-- Dropdown for Picklist -->
                    <apex:selectList value="{!row.selectedValue}" size="1" style="width: -webkit-fill-available;" rendered="{!row.fieldType == 'picklist'}" disabled="{!row.index > enabledRows}">
